public class Usre{
     private String id;
     private String name;
     private String pwd;
     private String desc;
     private Date createTime;
     private Date updateTime;
     public void getId(){
         return id;
     }
     public String getId(String id){
         this.id=id;
     }
     public void getName(){
          return name;
     }
     public String setName(String name){
         this.name=name;
     }
}
